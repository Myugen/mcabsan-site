{
  "version": 2,
  "routes": [
    {
      "src": "/api/mail.*",
      "dest": "api/mail/index.go"
    }
  ],
  "env": {
    "SENDGRID_API_KEY": "@sendgrid-api-key-production-v9bh",
    "DATO_API_KEY": "@dato-api-key-production-dkhc",
    "RECAPTCHA_SECRET_KEY": "@recaptcha-secret-key-production-pu1w",
    "GATSBY_RECAPTCHA_SITE_KEY": "@gatsby-recaptcha-site-key-production-f4no"
  },
  "build": {
    "env": {
      "GO_BUILD_FLAGS": "-ldflags '-s -w'"
    }
  }
}
